//未入力チェック
//最大文字数チェック
//最小文字数チェック
//バリテーション部分

//のバリデーション
if (!this.) {
    //空かどうかのバリテーション
    console.log("()");
    this.Validation.ErrMsg = ''
    return false
} else if(!valid(this.)){
    // 確認
    console.log("()が正しくありません");
    this.Validation.ErrMsg = ''
    return false
} else {
    //バリテーションがOKな場合
    console.log("のバリテーションOKです");
    //初期化
    this.Validation.ErrMsg = ""
    //結果の出力
    this.Result.Result = true;
}

